def list_creator(min, max, step):
    x = []
    for y in range(min, max + 1, step):
        if True:
            x.append(y)
    return x

min = int(input("Enter min value: "))
max = int(input("Enter max value: "))
step = int(input("Enter step: "))

result = list_creator(min, max, step)
print(result)
