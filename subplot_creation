def multiplots():
    
    x = np.linspace(0, 2*np.pi, 200)
    x1 = np.array([0,1,0,1])
    y = np.sin(x**2)
    y1 = np.array([-1,0,1,2])

    fig, ax = plt.subplots()
    ax.plot(x, y, 'r--')
    ax.plot(x1, y1, 'g--')
    plt.title('Simple plots')
    plt.xlabel("X axis")
    plt.ylabel("Y axis")
    plt.show()

multiplots()
